<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.diagnostics>
		<trace autoflush="true" indentsize="0">
			<listeners>
				<add type="Microsoft.WindowsAzure.Diagnostics.DiagnosticMonitorTraceListener, Microsoft.WindowsAzure.Diagnostics, Culture=neutral, PublicKeyToken=31bf3856ad364e35" name="AzureDiagnostics" />
				<add type="System.Diagnostics.TextWriterTraceListener" name="myListener" initializeData="C:\logs\myListener.log" /> 
			</listeners>
		</trace>
	</system.diagnostics>
</configuration>